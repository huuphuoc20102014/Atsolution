@model GenEf.Efs.Entities.Contact
@{
    ViewData["Title"] = "Details";
}



<h4>Chi tiết Liên hệ</h4>
<hr />

<div class="row form-group">
    <div class="col-md-3 text-md-right ">
    </div>
    <div class="col-md-7">
        <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Sửa</a>
        <a asp-action="Delete" asp-route-id="@Model.Id" class="btn btn-danger">Xoá</a>
        <a asp-action="Index" class="k-button">Quay lại danh sách</a>
    </div>
</div>


<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Tên :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.Name)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        @Html.DisplayNameFor(model => model.Email) :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.Email)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Số điện thoại :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.Phone)
    </div>
</div>


<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Tiêu đề :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.Title)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Nội dung :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.Body)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Đã đọc :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.IsRead)
    </div>
</div>


<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Ghi chú :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.Note)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Tạo bởi :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.CreatedBy)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Ngày tạo :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.CreatedDate)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Cập nhật bởi :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.UpdatedBy)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Ngày cập nhật :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.UpdatedDate)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        @Html.DisplayNameFor(model => model.RowVersion) :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.RowVersion)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        @Html.DisplayNameFor(model => model.RowStatus) :
    </div>
    <div class="col-md-7">
        @Model.RowStatus.ToString("N0")
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Bản đồ :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.Map)
    </div>
</div>



<div class="row form-group">
    <div class="col-md-3 text-md-right ">
    </div>
    <div class="col-md-7">
        <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Sửa</a>
        <a asp-action="Delete" asp-route-id="@Model.Id" class="btn btn-danger">Xoá</a>
        <a asp-action="Index" class="k-button">Quay lại danh sách</a>
    </div>
</div>
