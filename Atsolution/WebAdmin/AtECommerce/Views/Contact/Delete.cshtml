@model GenEf.Efs.Entities.Contact
@{
    ViewData["Title"] = "Delete";
}



<h4>Xóa Liên hệ</h4>
<h5 class="text-danger">Bạn có chắc chắn muốn xoá liên hệ này không?</h5>
<hr />

<form asp-action="Delete">
    <input type="hidden" name="Id" value="@Model?.Id" />
    <input type="hidden" asp-for="RowVersion" />
    <div class="row form-group">
        <div class="col-md-3 text-md-right ">
        </div>
        <div class="col-md-7">
            <input type="submit" value="Xóa" class="btn btn-danger" />
            <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Sửa</a>
            <a asp-action="Details" asp-route-id="@Model.Id" class="k-button">Chi tiết</a>
            <a asp-action="Index" class="k-button">Quay lại danh sách</a>
        </div>
    </div>
</form>



<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Tên :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.Name)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Email :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.Email)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Số điện thoại :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.Phone)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Tiêu đề :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.Title)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Nội dung :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.Body)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Được đọc :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.IsRead)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Ghi chú :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.Note)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Người tạo :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.CreatedBy)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Ngày tạo :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.CreatedDate)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Người sửa :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.UpdatedBy)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Ngày sửa :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.UpdatedDate)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        @Html.DisplayNameFor(model => model.RowVersion) :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.RowVersion)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        @Html.DisplayNameFor(model => model.RowStatus) :
    </div>
    <div class="col-md-7">
        @Model.RowStatus.ToString("N0")
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Bản đồ :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.Map)
    </div>
</div>



<form asp-action="Delete">
    <input type="hidden" name="Id" value="@Model?.Id" />
    <input type="hidden" asp-for="RowVersion" />
    <div class="row form-group">
        <div class="col-md-3 text-md-right ">
        </div>
        <div class="col-md-7">
            <input type="submit" value="Xóa" class="btn btn-danger" />
            <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Sửa</a>
            <a asp-action="Details" asp-route-id="@Model.Id" class="k-button">Chi tiết    </a>
            <a asp-action="Index" class="k-button">Quay lại danh sách</a>
        </div>
    </div>
</form>


